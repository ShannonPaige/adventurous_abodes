<h1 class="black-text">Rental Types</h1>

<div class="row">

<% @rental_types.each do |rental_type| %>
  <div class="col m4">
  <div class="card medium">
    <div class="card-image waves-effect waves-block waves-light">
      <%= image_tag "house-06.jpg" %>
    </div>
    <div class="card-content">
      <span class="card-title item-title activator grey-text text-darken-4"><%= link_to rental_type.name.pluralize, rental_type_path(slug: rental_type.slug), class: "btn" %>
</span>
    </div>
  </div>
</div>
<% end %>
</div>
